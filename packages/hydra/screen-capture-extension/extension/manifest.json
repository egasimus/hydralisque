{
  "name": "Screensharing Sample",
  "description": "Screensharing utility sample for getscreenmedia",
  "version": "0.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "icons": {
  },
  "permissions": [
    "desktopCapture"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [ {
    "js": [ "content.js" ],
    "matches": [ "*://localhost/*",  "*://ojack.github.io/*",  "*://ojack.github.io/*/*", "*://hydra-editor.glitch.me/*", "*://hydra-editor-v1.glitch.me/*",
      "*://192.168.0.164/*"]
  }],
  "externally_connectable": {
      "matches": [
          "https://simplewebrtc.com/*",
          "*://localhost/*",
          "*://192.168.0.164/*",
          "*://ojack.github.io/*",
          "*://hydra-editor.glitch.me/*",
          "*://hydra-editor-v1.glitch.me/*",
          "*://ojack.github.io/*/*"
      ]
  }
}
